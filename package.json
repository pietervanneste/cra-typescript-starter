{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "mobx": "^4.2.0",
    "mobx-react": "^5.0.0",
    "mobx-react-devtools": "^5.0.1",
    "mobx-react-router": "^4.0.2",
    "react": "^16.3.2",
    "react-app-rewire-hot-loader": "^1.0.1",
    "react-app-rewired": "^1.5.2",
    "react-dom": "^16.3.2",
    "react-intl": "^2.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.15.1"
  },
  "scripts": {
    "start": "react-app-rewired start --scripts-version react-scripts-ts",
    "build": "react-app-rewired build --scripts-version react-scripts-ts",
    "test": "react-app-rewired test --scripts-version react-scripts-ts --env=jsdom",
    "eject": "react-scripts eject",
    "intl": "NODE_ENV=development extract-messages -l=en,nl -o src/translations -d en --flat true 'src/**/!(*.test).js'"
  },
  "devDependencies": {
    "@types/history": "^4.6.2",
    "@types/jest": "^22.2.3",
    "@types/node": "^9.4.0",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/react-intl": "^2.3.8",
    "@types/react-router": "^4.0.24",
    "@types/react-router-dom": "^4.2.6",
    "@types/webpack-env": "^1.13.6",
    "extract-react-intl-messages": "^0.8.2",
    "prettier": "^1.12.1",
    "prettier-tslint": "^0.4.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.8.3"
  }
